{
  "compilerOptions": {
    "target": "es5",
    "lib": ["ES6"],
    "outDir": "./dir",
    "module": "commonjs",
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": ["node_modules", "dist"]
}
